{
  "name": "luxflux-openvpn",
  "version": "2.8.0",
  "author": "luxflux",
  "summary": "OpenVPN server puppet module",
  "license": "Apache-2.0",
  "source": "https://github.com/luxflux/puppet-openvpn",
  "project_page": "https://github.com/luxflux/puppet-openvpn",
